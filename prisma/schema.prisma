generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Prompt {
  id     Int    @id @default(autoincrement())
  text   String
  desc   String
  params Json?
}

model ContextDatabase {
  id          Int     @id @default(autoincrement())
  name        String
  description String
  scriptId    String
  scriptEnv   Json?
  rawDataLoc  String
  contextDBId String
}